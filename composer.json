{
    "name": "jxclsv/laravel-package-kickstarter",
    "description": "Basic Boilerplate for Laravel package development",
    "keywords": [
        "jxclsv",
        ":package_name"
    ],
    "homepage": "https://github.com/jxclsv/:package_name",
    "require": {
        "php": "^7.2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Justin",
            "email": "justinxclusive@gmail.com",
            "homepage": "https://github.com/jxclsv/:package_name",
            "role": "Tech Lead"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jxclsv\\LaravelPackageKickstarter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jxclsv\\LaravelPackageKickstarter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jxclsv\\LaravelPackageKickstarter\\KickstarterServiceProvider"
            ],
            "aliases": {
                "Kickstarter": "Jxclsv\\LaravelPackageKickstarter\\KickstarterFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
